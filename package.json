{
	"name": "rr",
	"private": true,
	"version": "0.0.0",
	"workspaces": [
		"./main.host",
		"./remote/widgets/wbs.host"
	],
	"scripts": {
		"setup": "scripts/nvm-use.sh && npm --prefix ./main.host i && npm --prefix ./remote/widgets/wbs.host i",
		"postinstall:deps": "npm --prefix ./main.host run build && npm --prefix ./remote/widgets/wbs.host run build",
		"serve:main-host": "npm --prefix ./main.host run dev",
		"serve:remote-widgets-wbs-host": "npm --prefix ./remote/widgets/wbs.host run dev",
		"build:main-host": "npm --prefix ./main.host run build",
		"build:remote-widgets-wbs-host": "npm --prefix ./remote/widgets/wbs.host run build",
		"preview:main-host": "npm --prefix ./main.host run preview",
		"preview:remote-widgets-wbs-host": "npm --prefix ./remote/widgets/wbs.host run preview"
	},
	"devDependencies": {}
}
